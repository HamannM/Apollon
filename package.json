{
  "name": "@ls1intum/apollon",
  "version": "2.0.4",
  "description": "A UML diagram editor.",
  "keywords": [],
  "homepage": "https://github.com/ls1intum/apollon#readme",
  "bugs": "https://github.com/ls1intum/apollon/issues",
  "license": "MIT",
  "author": {
    "name": "Stephan Krusche",
    "email": "krusche@in.tum.de",
    "url": "https://www1.in.tum.de/krusche"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": "github:ls1intum/apollon",
  "sideEffects": false,
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/webpack.dev.js",
    "build": "rm -rf dist && webpack --config ./webpack/webpack.prod.js",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:ts": "tslint --force -p tsconfig.json -c tslint.json -t stylish",
    "lint:css": "stylelint 'src/**/*.ts'",
    "prepare": "rm -rf lib && tsc -p .",
    "publish:patch": "yarn prepare && npm version patch && npm publish",
    "publish:minor": "yarn prepare && npm version minor && npm publish",
    "publish:major": "yarn prepare && npm version major && npm publish"
  },
  "dependencies": {
    "pepjs": "0.5.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "redux": "4.0.4",
    "redux-saga": "1.1.3",
    "redux-thunk": "2.3.0",
    "styled-components": "4.4.1",
    "tslib": "1.10.0",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@types/react": "16.9.4",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "7.1.3",
    "@types/styled-components": "4.4.1",
    "@types/uuid": "3.4.6",
    "circular-dependency-plugin": "5.2.0",
    "css-loader": "3.3.2",
    "html-webpack-plugin": "3.2.0",
    "style-loader": "1.0.1",
    "stylelint": "12.0.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-custom-processor-loader": "0.6.0",
    "stylelint-processor-styled-components": "1.9.0",
    "ts-loader": "6.2.1",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "tslint-react": "4.1.0",
    "typescript": "3.7.3",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "4.2.2"
  }
}
