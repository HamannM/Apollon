  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": "github:ls1intum/apollon",
  "sideEffects": false,
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/webpack.dev.js",
    "build": "rm -rf dist && webpack --config ./webpack/webpack.prod.js",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:ts": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:css": "stylelint 'src/**/*.ts'",
    "prepare": "rm -rf lib && tsc -p .",
    "update": "yarn upgrade-interactive --latest",
    "publish:patch": "yarn prepare && npm version patch && npm publish",
    "publish:minor": "yarn prepare && npm version minor && npm publish",
    "publish:major": "yarn prepare && npm version major && npm publish"
  },
  "dependencies": {
    "pepjs": "0.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "redux": "4.0.4",
    "redux-saga": "1.1.3",
    "redux-thunk": "2.3.0",
    "styled-components": "5.1.0",
    "tslib": "1.11.1",
    "uuid": "8.0.0"
  },
  "devDependencies": {
    "@types/react": "16.9.4",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "7.1.3",
    "@types/styled-components": "5.1.0",
    "@types/uuid": "7.0.3",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/eslint-plugin-tslint": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "circular-dependency-plugin": "5.2.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.5.3",
    "ejs-loader": "0.3.6",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jsdoc": "24.0.6",
    "eslint-plugin-prefer-arrow": "1.2.1",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "fork-ts-checker-notifier-webpack-plugin": "2.0.0",
    "fork-ts-checker-webpack-plugin": "4.1.3",
    "html-webpack-plugin": "4.3.0",
    "prettier": "2.0.5",
    "style-loader": "1.2.1",
    "stylelint": "13.3.3",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
