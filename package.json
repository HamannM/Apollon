{
  "name": "@ls1intum/apollon",
  "version": "2.0.14",
  "description": "A UML diagram editor.",
  "keywords": [],
  "homepage": "https://github.com/ls1intum/apollon#readme",
  "bugs": "https://github.com/ls1intum/apollon/issues",
  "license": "MIT",
  "author": {
    "name": "Stephan Krusche",
    "email": "krusche@in.tum.de",
